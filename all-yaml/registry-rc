apiVersion: v1
kind: ReplicationController
metadata:
  creationTimestamp: null
  generation: 1
  labels:
    name: registry-rc
  name: registry-rc
spec:
  replicas: 1
  selector:
    name: registry-apps
  template:
    metadata:
      creationTimestamp: null
      labels:
        name: registry-apps
    spec:
      containers:
      - image: vmware/registry:2.6.2-photon
        imagePullPolicy: IfNotPresent
        name: registry-app
        ports:
        - containerPort: 5000
          protocol: TCP
        - containerPort: 5001
          protocol: TCP
        resources: {}
        terminationMessagePath: /dev/termination-log
        volumeMounts:
        - mountPath: /etc/docker/registry
          name: config
      dnsPolicy: ClusterFirst
      restartPolicy: Always
      securityContext: {}
      terminationGracePeriodSeconds: 30
      volumes:
      - configMap:
          defaultMode: 420
          items:
          - key: config
            path: config.yml
          - key: cert
            path: root.crt
          name: harbor-registry-config
        name: config
status:
  replicas: 0
